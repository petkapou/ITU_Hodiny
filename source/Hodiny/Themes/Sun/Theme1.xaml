<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">

    <!-- Theme name as seen in ComboBox_Theme-->
    <sys:String x:Key="ThemeName">Téma 1</sys:String>

    <!-- Theme settings strings -->

    <!-- Type Image/Color -->
    <sys:String x:Key="Type">Image</sys:String>

    <!-- File name of image.  Is read only if Type is Image -->
    <sys:String x:Key="ImageFileName">Theme1.png</sys:String>
</ResourceDictionary>